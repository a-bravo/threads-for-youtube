{
  "name": "alientube",
  "version": "1.0.0",
  "description": "Reddit comments on Youtube",
  "main": "content_script.js",
  "scripts": {
    "start:firefox": "web-ext run --browser-console --start-url https://youtu.be/adzYW5DZoWs --start-url about:addons",
    "start:chrome": "web-ext run -t chromium --start-url https://youtu.be/adzYW5DZoWs",
    "start": "web-ext run -t firefox-desktop -t chromium --start-url https://youtu.be/adzYW5DZoWs",
    "clean": "rimraf dist",
    "prebuild": "npm run test",
    "build": "npm run clean && webpack --mode production --progress",
    "dev": "webpack --mode development --progress",
    "watch": "webpack --progress --watch",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-bravo/alientube.git"
  },
  "keywords": [
    "extension"
  ],
  "author": "a-bravo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-bravo/alientube/issues"
  },
  "homepage": "https://github.com/a-bravo/alientube#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.2.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "webextension-polyfill": "^0.5.0"
  }
}
